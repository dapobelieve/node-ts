{
   "compilerOptions": {
      "types": [
         "node"
      ],
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2016",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "inlineSourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true
   }
}